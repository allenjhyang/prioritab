{
  "name": "Prioritab",
  "description": "A new tab page to remind you of your priorities",
  "version": "3.0.0",
  "chrome_url_overrides": {
    "newtab": "prioritab.html"
  },
  "icons": {
    "16": "prioritab_logo_v2_16.png",
    "32": "prioritab_logo_v2_32.png",
    "48": "prioritab_logo_v2_48.png",
    "128": "prioritab_logo_v2_128.png"
  },
  "manifest_version": 3,
  "permissions": ["storage"],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}
